{
  "name": "Predator",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Get CV from website.",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "permissions": ["webNavigation", "tabs", "contextMenus", "<all_urls>"]
}
